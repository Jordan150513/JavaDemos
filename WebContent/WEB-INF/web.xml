  </welcome-file-list>
  
  <filter>
    <filter-name>PriviledgeFilter</filter-name>
    <filter-class>com.qiaodan.filter.PriviledgeFilter</filter-class>
    <init-param>
      <param-name>file</param-name>
      <param-value>/WEB-INF/privilege.properties</param-value>
    </init-param>
  </filter>
  <filter-mapping>
    <filter-name>PriviledgeFilter</filter-name>
    <url-pattern>*.do</url-pattern>
  </filter-mapping>
  
  <filter>
    <filter-name>ExceptionHandlerFileter</filter-name>
    <filter-class>com.qiaodan.filter.ExceptionHandlerFileter</filter-class>
  </filter>
  <filter-mapping>
    <filter-name>ExceptionHandlerFileter</filter-name>
    <url-pattern>/*</url-pattern>
  </filter-mapping>
  
  <filter>
    <filter-name>LogFilter</filter-name>
    <filter-class>com.qiaodan.filter.LogFilter</filter-class>
  </filter>
  <filter-mapping>
    <filter-name>LogFilter</filter-name>
    <url-pattern>/*</url-pattern>
  </filter-mapping>
  
  <filter>
    <filter-name>ImageRedirectFilter</filter-name>
    <filter-class>com.qiaodan.filter.ImageRedirectFilter</filter-class>
  </filter>
  <filter-mapping>
    <filter-name>ImageRedirectFilter</filter-name>
    <url-pattern>/image/goods/*</url-pattern>
  </filter-mapping>
  
  <filter>
    <filter-name>CharacterEncodingFilter</filter-name>
    <filter-class>com.qiaodan.filter.CharacterEncodingFilter</filter-class>
    <init-param>
      <param-name>characterEncoding</param-name>
      <param-value>UTF-8</param-value>
    </init-param>
    <init-param>
      <param-name>enable</param-name>
      <param-value>true</param-value>
    </init-param>
  </filter>
  <filter-mapping>
    <filter-name>CharacterEncodingFilter</filter-name>
    <url-pattern>/*</url-pattern>
  </filter-mapping>
  
